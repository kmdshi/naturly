{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web (Fixed Port)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--web-port=5500"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "naturly",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "naturly (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "naturly (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}